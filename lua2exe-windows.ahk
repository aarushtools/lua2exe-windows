browse := "HBITMAP:*" . Create_button_browse_png()
logo := "HBITMAP:*" . Create_lua2exeremovebgpreview_png()
convert := "HBITMAP:*" . Create_button_convert_png()
errorsign := "HBITMAP:*" . Create_download__3_removebgpreview_png()
back := "HBITMAP:*" . Create_button_back_png()
exit := "HBITMAP:*" . Create_exitremovebgpreview_png()
viewfull := "HBITMAP:*" . Create_button_viewfull_png()
checkmark := "HBITMAP:*" . Create_checkmark8256_png()
runbutton := "HBITMAP:*" . Create_button_run_png()
openbutton := "HBITMAP:*" . Create_button_open_png()
#SingleInstance, Force
Gui, Add, Picture, x127 y+10 w268 h259 vp1, %logo%
Gui, Add, Picture, x396 y276 w111 h29 gbrowseone vp2, %browse%
Gui, Add, Picture, x396 y333 w111 h29 gbrowsetwo vp3, %browse%
Gui, Add, Picture, x175 y372 w182 h48 gconvert vcon, %convert%
Gui, Font, S15 CDefault, Bahnschrift
Gui, Add, Edit, x21 y276 w364 h28 , C:\
Gui, Add, Edit, x21 y333 w364 h28 , C:\
Gui, -Caption
Gui, Add, Picture, h50 w50 y-5 x+105 gGuiClose vclose, %exit%
Gui, +hwndHgui
Gui, Show, w536 h448, Lua 2 EXE
OnMessage(0x201, "WM_LBUTTONDOWN")
return

GuiClose:
winfade("ahk_id " Hgui,10,10)
ExitApp

WM_LBUTTONDOWN()
{
If A_Gui
PostMessage, 0xA1, 2
}

browseone:
FileSelectFile, browseoneloc, 3, %A_WorkingDir%, Select Source File, Lua Source Files (*.lua)
if (browseoneloc = "")
	return
GuiControl, Text, Edit1, %browseoneloc%
return
browsetwo:
FileSelectFile, browsetwoloc, S26, %A_WorkingDir%, Select Output Executable Name and Location, Executable Files (*.exe)
if (browsetwoloc = "")
	return
IfNotInString, browsetwoloc, .exe
{
	browsetwoloc := browsetwoloc ".exe"
}
GuiControl, Text, Edit2, %browsetwoloc%
return

convert:
if (browseoneloc = "" or browseoneloc = "C:\")
{
	msgerror = Invalid path for input lua file.
	goto errorcaught
}
if (browsetwoloc = "" or browsetwoloc = "C:\")
{
	msgerror = Invalid path for output exe.
	goto errorcaught
}
IfNotExist, %browseoneloc%
{
	msgerror = Input lua file does not exist.
	goto errorcaught
}
IfExist, %browsetwoloc%
{
	FileDelete, %browsetwoloc%
}
RunWait, %comspec% /c %A_WorkingDir%\bin\glue.exe %A_WorkingDir%\bin\srlua.exe %browseoneloc% %browsetwoloc%,, Hide
if ErrorLevel
{
	msgerror = An error happened while running the command %A_WorkingDir%\bin\glue.exe %A_WorkingDir%\bin\srlua.exe %browseoneloc% %browsetwoloc%
	goto errorcaught
} else {
	goto success
}
return

errorcaught:
GuiControl, Hide, Edit1
GuiControl, Hide, Edit2
GuiControl, Hide, p1
GuiControl, Hide, p2
GuiControl, Hide, p3
GuiControl, Hide, con
Gui, Add, Picture, x175 y7 w182 h182 , %errorsign%
Gui, Font, S40 CDefault, Century Gothic
Gui, Add, Text, x108 y189 w326 h67 , Error Caught
Gui, Font, S30 CDefault, Century Gothic
Gui, Add, Text, x98 y256 w364 h48 , More Information:
Gui, Font, S12 CDefault, Century Gothic
Gui, Add, Text, x38 y314 w350 h45 , %msgerror%
Gui, Add, Picture, x190 y362 w147 h66 gback, %back%
StringLen, msgerrorlength, msgerror
if (msgerrorlength > 150) {
Gui, Add, Picture, x400 y318 w102 h33 gviewfull, %viewfull%
}
return

viewfull:
MsgBox, 16, Full Error, %msgerror%
return

back:
Reload
return

success:
GuiControl, Hide, Edit1
GuiControl, Hide, Edit2
GuiControl, Hide, p1
GuiControl, Hide, p2
GuiControl, Hide, p3
GuiControl, Hide, con
Gui, Add, Picture, x175 y7 w182 h182 , %checkmark%
Gui, Font, S40 CDefault, Century Gothic
Gui, Add, Text, x108 y189 w326 h67 , Success!
Gui, Font, S30 CDefault, Century Gothic
Gui, Add, Text, x98 y256 w364 h48 , More Information:
Gui, Font, S7 CDefault, Century Gothic
Gui, Add, Text, x38 y314 w350 h45 , Executable located at %browsetwoloc%
Gui, Add, Picture, x190 y362 w147 h66 gback, %back%
Gui, Add, Picture, x400 y318 w77 h33 gopenloc, %openbutton%
Gui, Add, Picture, x400 y368 w77 h33 grunprog, %runbutton%
return

openloc:
Run, %browsetwoloc%\..\
return

runprog:
Run, %comspec% /k cd %browsetwoloc%\..\
return

winfade(w:="",t:=128,i:=1,d:=10) { ; by joedf  -  http://ahkscript.org/boards/viewtopic.php?f=6&t=512
	w:=(w="")?("ahk_id " WinActive("A")):w
	t:=(t>255)?255:(t<0)?0:t
	WinGet,s,Transparent,%w%
	s:=(s="")?255:s ;prevent trans unset bug
	WinSet,Transparent,%s%,%w% 
	i:=(s<t)?abs(i):-1*abs(i)
	while(k:=(i<0)?(s>t):(s<t)&&WinExist(w)) {
		WinGet,s,Transparent,%w%
		s+=i
		WinSet,Transparent,%s%,%w%
		sleep %d%
	}
}

